            },
            "asset": {
              "type": "string",
              "example": "BTC"
            },
            "indicators": {
              "type": "array",
              "items": {
                "type": "string",
                "enum": [
                  "rsi",
                  "macd",
                  "volume",
                  "fear_greed"
                ]
              },
              "description": "Optional list of indicators to include"
            }
          },
          "required": [
            "protocol",
            "timeframe",
            "asset"
          ]
        },
        "response_schema": {
          "type": "object",
          "properties": {
            "summary": {
              "type": "string"
            },
            "sentiment": {
              "type": "string",
              "enum": [
                "bullish",
                "bearish",
                "neutral"
              ]
            },
            "confidence": {
              "type": "number",
              "minimum": 0,
              "maximum": 1
            },
            "indicators": {
              "type": "object"
            }
          },
          "required": [
            "summary",
            "sentiment",
            "confidence"
